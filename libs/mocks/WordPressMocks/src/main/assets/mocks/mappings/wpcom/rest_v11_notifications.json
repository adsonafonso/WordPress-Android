{
    "request": {
        "method": "GET",
        "urlPath": "/rest/v1.1/notifications",
        "queryParameters": {
            "number": {
                "matches": "[0-9]+"
            },
            "num_note_items": {
                "matches": "[0-9]+"
            },
            "fields": {
                "equalTo": "id,type,unread,body,subject,timestamp,meta"
            }
        }
    },
    "response": {
        "status": 200,
        "jsonBody": {
            "last_seen_time": "1550137757",
            "number": 7,
            "notes": []
        },
        "headers": {
            "Content-Type": "application/json",
            "Connection": "keep-alive",
            "Cache-Control": "no-cache, must-revalidate, max-age=0"
        }
    }
}